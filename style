(defun C:SetRowsToData ()
  ;; Get the active document
  (setq doc (vla-get-ActiveDocument (vlax-get-Acad-Object)))

  ;; Loop through all entities in the active document
  (vlax-for entity (vla-get-ModelSpace doc)
    ;; Check if the entity is a table
    (if (eq (vla-get-ObjectName entity) "AcDbTable")
      (progn
        ;; Get the number of rows and columns in the table
        (setq numRows (vla-get-Rows entity))
        (setq numCols (vla-get-Columns entity))

        ;; Loop through each cell in the table
        (setq row 0)
        (while (< row numRows)
          (setq col 0)
          (while (< col numCols)
            ;; Modify each cell to match "Data" style

            ;; Change text style to "Standard" (or the style you prefer for "Data")
            (vla-SetTextStyle entity row col "Standard")

            ;; Reset text height (adjust to your standard "Data" height)
            (vla-SetTextHeight entity row col 0.18)

            ;; Reset text alignment (usually centered for data rows)
            (vla-SetAlignment entity row col acMiddleCenter)

            ;; Move to the next column
            (setq col (1+ col))
          )
          ;; Move to the next row
          (setq row (1+ row))
        )
      )
    )
  )
  (princ "\nAll table rows have been set to 'Data' style properties.")
  (princ)
)

(princ "\nType 'SetRowsToData' to reset all table rows to the 'Data' style properties.\n")
(princ)
